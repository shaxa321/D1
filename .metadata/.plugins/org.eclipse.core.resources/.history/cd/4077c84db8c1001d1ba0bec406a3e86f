package todayExcersices;

public class LastEx {
	
	static float perimetroRettangolo(float base,float altezza) {
		return (base*altezza);
	}
	
	public static void main(String args[]) {
		float base=4.64f;
		float altezza=3.25f;
		System.out.println("L'altezza Ã¨ ");
		System.out.println(perimetroRettangolo(base,altezza));
	}
}
